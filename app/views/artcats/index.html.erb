<p id="notice"><%= notice %></p>

<h1>Listing Artcats</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>Path</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @artcats.each do |artcat| %>
      <tr>
        <td><%= artcat.description %></td>
        <td><%= artcat.path %></td>
        <td><%= link_to 'Show', artcat %></td>
        <td><%= link_to 'Edit', edit_artcat_path(artcat) %></td>
        <td><%= link_to 'Destroy', artcat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Artcat', new_artcat_path %>
